<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<general>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.sheet_general
					</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<images>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.images</label>
							<config>
								<type>group</type>
								<internal_type>file</internal_type>
								<allowed>files</allowed>
								<size>6</size>
								<maxitems>1000</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
								<allowed>gif,jpg,jpeg,png</allowed>
								<max_size>10240</max_size>
								<uploadfolder>uploads/tx_jfheaderslide</uploadfolder>
							</config>
						</TCEforms>
					</images>
					<href>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.href</label>
							<config>
								<type>text</type>
								<wrap>OFF</wrap>
								<cols>48</cols>
								<rows>6</rows>
							</config>
						</TCEforms>
					</href>
					<caption>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.caption</label>
							<config>
								<type>text</type>
								<wrap>OFF</wrap>
								<cols>48</cols>
								<rows>6</rows>
							</config>
						</TCEforms>
					</caption>
				</el>
			</ROOT>
		</general>
		<settings>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.sheet_settings
					</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<width>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.width</label>
							<config>
								<type>input</type>
								<size>5</size>
								<max>4</max>
								<checkbox>0</checkbox>
								<eval>int</eval>
							</config>
						</TCEforms>
					</width>
					<height>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.height</label>
							<config>
								<type>input</type>
								<size>5</size>
								<max>4</max>
								<checkbox>0</checkbox>
								<eval>int</eval>
							</config>
						</TCEforms>
					</height>
					<showcontroller>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.showcontroller</label>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>
					</showcontroller>
					<showcaption>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.showcaption</label>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>
					</showcaption>
					<captionsDuration>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.captionsDuration</label>
							<config>
								<type>input</type>
								<size>5</size>
								<max>4</max>
								<checkbox>0</checkbox>
								<default>500</default>
								<eval>int</eval>
							</config>
						</TCEforms>
					</captionsDuration>
					<random>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.random</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</random>
					<noJS>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.noJS</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</noJS>
					<animateOneImage>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.animateOneImage</label>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>
					</animateOneImage>
					<loaderAnimation>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.loaderAnimation</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</loaderAnimation>
					<centerImage>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.centerImage</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</centerImage>
				</el>
			</ROOT>
		</settings>
		<movement>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.sheet_movement
					</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<type>
						<TCEforms>
							<onChange>reload</onChange>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.type</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">fade</numIndex>
										<numIndex index="1">fade</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">push</numIndex>
										<numIndex index="1">push</numIndex>
									</numIndex>
<!--
this does not work since 1.3.1 :(
									<numIndex index="3" type="array">
										<numIndex index="0">wipe</numIndex>
										<numIndex index="1">wipe</numIndex>
									</numIndex>
-->
									<numIndex index="4" type="array">
										<numIndex index="0">flash</numIndex>
										<numIndex index="1">flash</numIndex>
									</numIndex>
									<numIndex index="5" type="array">
										<numIndex index="0">fold</numIndex>
										<numIndex index="1">fold</numIndex>
									</numIndex>
									<numIndex index="6" type="array">
										<numIndex index="0">kenburns</numIndex>
										<numIndex index="1">kenburns</numIndex>
									</numIndex>
								</items>
								<size>1</size>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</type>
					<typedirection>
						<TCEforms>
							<exclude>1</exclude>
							<displayCond>FIELD:type:=:wipe</displayCond>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.typedirection</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">left</numIndex>
										<numIndex index="1">left</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">right</numIndex>
										<numIndex index="1">right</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">down</numIndex>
										<numIndex index="1">down</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">up</numIndex>
										<numIndex index="1">up</numIndex>
									</numIndex>
								</items>
								<size>1</size>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</typedirection>
					<pan>
						<TCEforms>
							<exclude>1</exclude>
							<displayCond>FIELD:type:=:kenburns</displayCond>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.pan</label>
							<config>
								<type>input</type>
								<size>5</size>
								<max>3</max>
								<range>
									<lower>0</lower>
									<upper>100</upper>
								</range>
								<checkbox>0</checkbox>
								<eval>int</eval>
							</config>
						</TCEforms>
					</pan>
					<zoom>
						<TCEforms>
							<exclude>1</exclude>
							<displayCond>FIELD:type:=:kenburns</displayCond>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.zoom</label>
							<config>
								<type>input</type>
								<size>5</size>
								<max>3</max>
								<range>
									<lower>0</lower>
									<upper>100</upper>
								</range>
								<checkbox>0</checkbox>
								<eval>int</eval>
							</config>
						</TCEforms>
					</zoom>
					<transition>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.transition</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">quad</numIndex>
										<numIndex index="1">quad</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">cubic</numIndex>
										<numIndex index="1">cubic</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">quart</numIndex>
										<numIndex index="1">quart</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">quint</numIndex>
										<numIndex index="1">quint</numIndex>
									</numIndex>
									<numIndex index="5" type="array">
										<numIndex index="0">expo</numIndex>
										<numIndex index="1">expo</numIndex>
									</numIndex>
									<numIndex index="6" type="array">
										<numIndex index="0">circ</numIndex>
										<numIndex index="1">circ</numIndex>
									</numIndex>
									<numIndex index="7" type="array">
										<numIndex index="0">sine</numIndex>
										<numIndex index="1">sine</numIndex>
									</numIndex>
									<numIndex index="8" type="array">
										<numIndex index="0">back</numIndex>
										<numIndex index="1">back</numIndex>
									</numIndex>
									<numIndex index="9" type="array">
										<numIndex index="0">bounce</numIndex>
										<numIndex index="1">bounce</numIndex>
									</numIndex>
									<numIndex index="10" type="array">
										<numIndex index="0">elastic</numIndex>
										<numIndex index="1">elastic</numIndex>
									</numIndex>
								</items>
								<size>1</size>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</transition>
					<transitiondir>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.transitiondir</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0"></numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">:in</numIndex>
										<numIndex index="1">:in</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">:out</numIndex>
										<numIndex index="1">:out</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">:in:out</numIndex>
										<numIndex index="1">:in:out</numIndex>
									</numIndex>
								</items>
								<size>1</size>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</transitiondir>
					<transitionduration>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.transitionduration</label>
							<config>
								<type>input</type>
								<size>5</size>
								<max>5</max>
								<checkbox>0</checkbox>
								<eval>int</eval>
							</config>
						</TCEforms>
					</transitionduration>
					<displayduration>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.displayduration</label>
							<config>
								<type>input</type>
								<size>5</size>
								<max>5</max>
								<checkbox>0</checkbox>
								<eval>int</eval>
							</config>
						</TCEforms>
					</displayduration>
					<fastOnLoad>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.fastOnLoad</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</fastOnLoad>
					<color>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:jf_headerslide/locallang_db.xml:tt_content.pi_flexform.color</label>
							<config>
								<type>input</type>
								<size>30</size>
								<max>255</max>
								<checkbox>0</checkbox>
							</config>
						</TCEforms>
					</color>
				</el>
			</ROOT>
		</movement>
	</sheets>
</T3DataStructure>
